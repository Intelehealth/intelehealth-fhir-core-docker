{
  "name": "intelehealth-webapp",
  "version": "0.0.0",
  "scripts": {
    "config-dev": "ts-node ./src/assets/scripts/setEnv.ts --environment=dev",
    "config-prod": "ts-node ./src/assets/scripts/setEnv.ts --environment=prod",
    "config-test": "ts-node ./src/assets/scripts/setEnv.ts --environment=test",
    "ng": "ng",
    "ng-host": "ng s --host 0.0.0.0 --disableHostCheck",
    "start": "npm run config-dev && ng serve",
    "build": "npm run config-prod && ng build --configuration production --aot",
    "test": "ng test --include src/app/session",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,css}": "tslint --fix -c ./tslint.json"
  },
  "private": true,
